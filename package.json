{
    "name": "my-react-learning-app",
    "version": "1.0.0",
    "private": true,
    "description": "A simple React application that demonstrates basic concepts like JSX, props, and state, starting with a 'Hello World' example.",
    "keywords": [
      "React",
      "JSX",
      "Props",
      "State",
      "Learning",
      "Counter",
      "Hello World"
    ],
    "homepage": "http://localhost:3000",
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/my-react-learning-app.git"
    },
    "license": "MIT",
    "author": "Your Name <your.email@example.com>",
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "jest --watch",
      "test:ci": "jest --ci --coverage"
    },
    "dependencies": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0",
      "react-scripts": "5.0.1"
    },
    "devDependencies": {
      "@testing-library/jest-dom": "^5.17.0",
      "@testing-library/react": "^13.3.0",
      "@testing-library/user-event": "^14.1.0",
      "jest": "^29.0.0",
      "jest-environment-jsdom": "^29.0.0"
    },
    "jest": {
      "testEnvironment": "jsdom",
      "setupFilesAfterEnv": [
        "<rootDir>/src/setupTests.js"
      ],
      "collectCoverageFrom": [
        "src/**/*.js",
        "!src/index.js",
        "!src/reportWebVitals.js"
      ],
      "coverageDirectory": "coverage"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }
